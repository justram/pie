{
	"name": "pie",
	"version": "0.1.0",
	"description": "Structured extraction library and CLI built on @mariozechner/pi-ai.",
	"license": "MIT",
	"keywords": [
		"llm",
		"structured-data",
		"extraction",
		"cli",
		"schema",
		"pi-ai"
	],
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./cache": {
			"types": "./dist/cache/index.d.ts",
			"default": "./dist/cache/index.js"
		}
	},
	"bin": {
		"pie": "./bin/pie"
	},
	"files": [
		"bin",
		"dist",
		"README.md",
		"LICENSE",
		"CHANGELOG.md"
	],
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"typecheck": "tsc -p tsconfig.base.json --noEmit",
		"lint": "biome check .",
		"lint:imports": "eslint --fix \"{src,test,spec,examples}/**/*.ts\"",
		"check": "biome check --write . && npm run typecheck",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:e2e": "vitest run --config vitest.config.e2e.ts",
		"prepare": "husky"
	},
	"dependencies": {
		"@mariozechner/pi-ai": "^0.49.2",
		"@silvia-odwyer/photon-node": "^0.3.4",
		"@sinclair/typebox": "^0.34.46",
		"minijinja-js": "^2.14.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/node": "^22.10.2",
		"@typescript-eslint/parser": "^8.46.0",
		"eslint": "^9.36.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"fast-check": "^4.5.3",
		"husky": "^9.1.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.16"
	}
}
